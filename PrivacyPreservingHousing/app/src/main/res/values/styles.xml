<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="MaterialAppTheme.Base">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/deep_purple_500</item>
        <item name="colorPrimaryDark">@color/deep_purple_900</item>
        <item name="colorAccent">@color/deep_purple_50</item>
        <item name="android:colorAccent">@color/accent_material_light</item>
        <item name="android:colorPrimary">@color/primary_material_light</item>
    </style>
    <style name="MaterialAppTheme.Base" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorAccent">@color/accent</item>
    </style>

</resources>
